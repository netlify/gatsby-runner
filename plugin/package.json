{
  "name": "@netlify/gatsby-runner",
  "version": "1.1.7",
  "description": "CLI runner for Gatsby",
  "bin": {
    "gatsby-runner": "dist/cli.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./handler": "./dist/handler.js"
  },
  "files": [
    "dist",
    "manifest.yml"
  ],
  "license": "MIT",
  "private": false,
  "dependencies": {
    "@netlify/functions": "^1.0.0",
    "chalk": "^4.1.2",
    "execa": "^5.0.0",
    "fastq": "^1.11.0",
    "fs-extra": "^10.0.0"
  },
  "devDependencies": {
    "@netlify/build": "^27.1.2",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^17.0.34",
    "gatsby-core-utils": "^3.14.0",
    "gatsby-plugin-sharp": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "release-it": "^15.0.0",
    "rimraf": "^3.0.2",
    "tsup": "^5.12.8"
  },
  "peerDependencies": {
    "gatsby-core-utils": "*",
    "gatsby-plugin-sharp": "*"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build:lib": "tsup-node src/index.ts src/handler.ts --target node14  --dts",
    "build:cli": "tsup-node src/cli.ts --target node14",
    "build": "npm-run-all -s clean -p build:*",
    "release-it": "release-it --verbose",
    "release": "run-s build release-it",
    "prepublishOnly": "npm run build"
  },
  "publishConfig": {
    "access": "public"
  },
  "release-it": {
    "npm": {
      "skipChecks": true
    }
  }
}

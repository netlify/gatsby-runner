{
  "name": "@netlify/gatsby-runner",
  "version": "1.0.3",
  "description": "CLI runner for Gatsby",
  "bin": {
    "gatsby-runner": "dist/cli.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "private": false,
  "dependencies": {
    "@netlify/functions": "^1.0.0",
    "execa": "^5.0.0",
    "fastq": "^1.11.0",
    "fs-extra": "^10.0.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^17.0.34",
    "gatsby-plugin-sharp": "^4.14.1",
    "release-it": "^15.0.0",
    "tsup": "^5.12.8"
  },
  "peerDependencies": {
    "gatsby-plugin-sharp": "*"
  },
  "scripts": {
    "build": "tsup-node src/index.ts src/cli.ts --target node14 --dts src/index.ts",
    "release": "release-it"
  },
  "publishConfig": {
    "access": "public"
  }
}
